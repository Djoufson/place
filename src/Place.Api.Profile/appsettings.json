{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "app": {
    "name": "Place Profile Api"
  },
  "swagger": {
    "enabled": true,
    "reDocEnabled": false,
    "name": "v1",
    "title": "Profile Api",
    "version": "v1",
    "routePrefix": "docs"
  },
  "Postgres": {
    "Host": "localhost",
    "Port": 5489,
    "Username": "postgres",
    "Password": "postgres",
    "Database": "PlaceApiProfile"
  },
  "AllowedHosts": "*",
  "logger": {
    "applicationName": "orders-service",
    "excludePaths": ["/ping", "/metrics"],
    "level": "information",
    "console": {
      "enabled": true
    },
    "file": {
      "enabled": true,
      "path": "logs/logs.txt",
      "interval": "day"
    },
    "seq": {
      "enabled": true,
      "url": "http://localhost:5341",
      "token": "secret"
    }
  }
}
